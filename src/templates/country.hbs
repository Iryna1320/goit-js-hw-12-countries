{{#each this}}
<h1 class="name-countries">{{name}}</h1>
<div class="card">
  <div class="text-country">
    <span class="text">Capital: <span>{{capital}}</span></span>
    <span class="text">Population: <span>{{population}}</span></span>
    <span class="text">Languages: </span>
    <ul class="list-group">
      {{#each languages}}
      <li class="list-group-item"> {{this.name}}</li>
      {{/each}}
    </ul>
  </div>
  <div class="flag-countries">
    <img src="{{flag}}" width="200" height="200" alt="{{name}}">
  </div>
</div>
{{/each}}
